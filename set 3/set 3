#include <iostream>
using namespace std;

int main()
{
    int a{};
    cout << "Please Enter the Celsius: " << endl;
    cin << a;
    double result = 1.8 * a + 32;
    cout << "Celsius value: " << a << endl;
    cout << "Fahrenheit" << result << endl;
    return 0;
}